<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:itheima="http://schemas.android.com/apk/res/com.itheima17.mobileguard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <!-- 设置中心标题 -->

    <TextView
        style="@style/title_style"
        android:text="设置中心" />

    <com.itheima17.mobileguard.view.SettingCenterItemView
        android:id="@+id/sciv_settingcenter_autoupdate"
        android:layout_marginTop="10dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        itheima:desc="自动更新设置"
        itheima:bgtype="first" />

    <com.itheima17.mobileguard.view.SettingCenterItemView
        android:id="@+id/sciv_settingcenter_blackservice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        itheima:desc="黑名单拦截设置"
        itheima:bgtype="last" />

    <com.itheima17.mobileguard.view.SettingCenterItemView
        android:id="@+id/sciv_settingcenter_cominglocation"
        android:layout_marginTop="10dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        itheima:desc="电话归属地设置"
        itheima:bgtype="first" />
    
     <com.itheima17.mobileguard.view.SettingCenterItemView
        android:id="@+id/sciv_settingcenter_locationstyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        itheima:desc="归属地样式"
        itheima:isToggleShow="false"
        itheima:bgtype="last" />

    <!--
  <include layout="@layout/settingcenter_item_view1"/>
  <include layout="@layout/settingcenter_item_view"/>
  <include layout="@layout/settingcenter_item_view2"/>
    -->

</LinearLayout>